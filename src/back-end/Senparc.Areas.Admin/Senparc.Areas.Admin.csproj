<Project Sdk="Microsoft.NET.Sdk.Razor">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<!--<TargetFramework>net5.0</TargetFramework>-->
		<!--<RazorLangVersion>2.0</RazorLangVersion>-->
		<GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
		<AddRazorSupportForMvc>true</AddRazorSupportForMvc>
		<DocumentationFile>../Senparc.Web/App_Data/ApiDocXML/Senparc.Areas.Admin.XML</DocumentationFile>
		<Configurations>Debug;Release;Debug-Dapr</Configurations>
	</PropertyGroup>

	<ItemGroup>
		<!--<FrameworkReference Include="Microsoft.AspNetCore.App" />-->

		<EmbeddedResource Include="wwwroot\**\*" />

		<Compile Remove="Domain\Migrations\Oracle\**" />

		<Content Remove="Domain\Migrations\Oracle\**" />

		<EmbeddedResource Remove="Domain\Migrations\Oracle\**" />

		<None Remove="Domain\Migrations\Oracle\**" />
		<PackageReference Include="Microsoft.AspNetCore.Authorization" Version="8.0.0" />
		<PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="8.0.0" />
	</ItemGroup>

	<ItemGroup>
		<Content Remove="wwwroot\css\Admin\TenantInfo\Index.css" />
		<Content Remove="wwwroot\css\Admin\SystemConfig\Index.css" />
		<Content Remove="wwwroot\js\Admin\Pages\SystemConfig\Index.js" />
		<Content Remove="wwwroot\js\Admin\Pages\TenantInfo\Index.js" />
		<Content Remove="wwwroot\js\Admin\Pages\XncfModule\start.js" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="8.0.0" />
		<PackageReference Include="Senparc.CO2NET.Cache.CsRedis" Version="1.2.2" />
		<PackageReference Include="Senparc.CO2NET.Cache.Redis" Version="4.2.2" />
		<PackageReference Include="Senparc.Ncf.AreaBase" Version="0.18.1.2-preview1" />
		<PackageReference Include="Senparc.Ncf.Core" Version="0.19.1.2-preview1" />
		<PackageReference Include="Senparc.Ncf.Log" Version="0.18.1.2-preview1" />
		<PackageReference Include="Senparc.Ncf.Mvc.UI" Version="0.18.1.2-preview1" />
		<PackageReference Include="Senparc.Ncf.Service" Version="0.19.1.2-preview1" />
		<PackageReference Include="Senparc.Ncf.Utility" Version="0.18.1.2-preview1" />
		<PackageReference Include="Senparc.Ncf.XncfBase" Version="0.20.1.4-preview1" />

		<PackageReference Include="Senparc.Xncf.AreasBase" Version="0.9.1.2-preview1" />
		<PackageReference Include="Senparc.Xncf.SystemCore" Version="0.10.1.2-preview1" />
		<PackageReference Include="Senparc.Xncf.SystemManager" Version="0.10.1.2-preview1" />
		<PackageReference Include="Senparc.Xncf.XncfModuleManager" Version="0.11.1.2-preview1" />
		<PackageReference Include="Senparc.Xncf.Tenant" Version="0.11.1.3-preview1" />
		<PackageReference Include="Senparc.Xncf.WeixinManager" Version="0.16.1-preview1" />

